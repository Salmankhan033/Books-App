{
  "expo": {
    "name": "WeAreNova",
    "slug": "WeAreNova",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./src/assets/bookLogo.png",
    "splash": {
      "image": "./src/assets/logoWhite.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFA500"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    }
  }
}
